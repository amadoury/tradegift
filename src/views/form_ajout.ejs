<div class="container">
    <h2 class="mt-5 mb-3 myfont-title">Ajout de Client</h2>
    <form action="/add_cliente" method="post">
      <div class="form-group">
        <label class="myfont" for="pseudo">Pseudo :</label>
        <input type="text" class="form-control" name="pseudo" id="pseudo" placeholder="Entrez votre pseudo" required>
      </div>
      <div class="form-group">
        <label class="myfont" for="motDePasse">Mot de passe :</label>
        <input type="password" class="form-control" name="password"  id="motDePasse" placeholder="Entrez votre mot de passe" required>
      </div>
      <button type="submit" class="btn mybtn">Ajouter</button>
    </form>
</div>